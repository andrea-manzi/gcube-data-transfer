                  persistence-modifier="persistent"/>
            <field
                  name="size"
                  persistence-modifier="persistent"/>
            <field
                  name="status"
                  persistence-modifier="persistent"/>
            <field
                  name="outcome"
                  persistence-modifier="persistent"/>
            <field
                  name="transferType"
                  persistence-modifier="persistent"/>
            <field
                  name="transferID"
                  persistence-modifier="persistent"/>
            <field
                  name="transferTime"
                  persistence-modifier="persistent"/>
            <field
                  name="bytesOfObjTransferred"
                  persistence-modifier="persistent"/>
        </class>
    </package>
</jdo>